CREATE TABLE IF NOT EXISTS DbTodoGroup (
  `id` TEXT NOT NULL,
  `name` TEXT NOT NULL,
  PRIMARY KEY(`id`)
);

getAll:
SELECT * FROM DbTodoGroup;

save:
INSERT INTO DbTodoGroup(id, name)
VALUES (?, ?);

rename:
UPDATE DbTodoGroup
SET name=?
WHERE id=?;

delete:
DELETE FROM DbTodoGroup WHERE id=?;

deleteAll:
DELETE FROM DbTodoGroup;
